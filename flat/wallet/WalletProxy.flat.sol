 *
 * We often need to pass calls around, so this is a common representation to use.
 */
    struct Call {
        address to;
        bytes callData;
        uint256 value;
    }

    struct Execution {
        address target;
        uint256 value;
        bytes callData;
    }

/// @notice Metadata to splice a return value into a call.
    struct ReturnDataLink {
